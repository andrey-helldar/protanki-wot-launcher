        <Button x:Name="bClose" Content="Закрыть" HorizontalAlignment="Right" Margin="0,0,10,30" VerticalAlignment="Bottom" Click="Button_Click" Style="{DynamicResource ButtonCLOSE}" Grid.Row="1"/>
